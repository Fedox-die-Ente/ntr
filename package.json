{
  "name": "native-translate-resource",
  "displayName": "NativeTranslate Resource",
  "description": "A simple and new minimalistic language file format.",
  "publisher": "Fedox",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fedox-die-Ente/ntr-vscode"
  },
  "author": {
    "name": "Fedox",
    "email": "f3dox@proton.me"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ntr",
      "aliases": ["NativeTranslate-Resource", "ntr"],
      "extensions": [".ntr"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ntr",
      "scopeName": "source.ntr",
      "path": "./syntaxes/ntr.tmLanguage.json"
    }]
  }
}